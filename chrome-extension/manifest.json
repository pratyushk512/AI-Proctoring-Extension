{
    "manifest_version": 3,
    "name": "AI-Proctor-Extension",
    "version": "1.0",
    "description": "Ai Proctoring System to reduce malpratices during online contest",
   "permissions": ["activeTab","clipboardRead",
    "clipboardWrite"],
    "host_permissions": ["https://*/*"],

    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }
  